{
  "name": "Freshbox",
  "type": "module",
  "version": "1.0.0",
  "description": "Freshbox",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node backend/server",
    "server": "nodemon --exec babel-node backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/utils/seeder.js",
    "data:destroy": "node backend/utils/seeder.js -d"
  },
  "browser": {
    "http": false
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "react-ecommerce-material": "file:frontend",
    "stripe": "^8.129.0"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-plugin-react": "^7.21.2"
  }
}
